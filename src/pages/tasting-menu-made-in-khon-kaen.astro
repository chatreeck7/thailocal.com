---
import Layout from '../layouts/Layout.astro';
import Logo from "../assets/amazing-thailand.png"
import iKaen from "../assets/IKaen.jpg"
import iKaen1 from "../assets/IKaen1.jpg"
import iKaen2 from "../assets/IKaen2.jpg"

import srcImage from "../assets/KhonKaenTrip.jpeg"
import Trip_3 from "../assets/KhonKaenTrip_2.jpg"
import Trip_4 from "../assets/KhonKaenTrip_3.jpg"
import Trip_5 from "../assets/KhonKaenTrip_5.jpg"

import Trip_6 from "../assets/KhonKaenTrip_6.jpg"
import Trip_7 from "../assets/KhonKaenTrip_7.jpg"
import Trip_8 from "../assets/KhonKaenTrip_8.jpg"

import Trip_9 from "../assets/KhonKaenTrip_9.jpg"
import Trip_10 from "../assets/KhonKaenTrip_10.jpg"
import Trip_11 from "../assets/KhonKaenTrip_11.jpg"

import Trip_12 from "../assets/KhonKaenTrip_12.jpg"
