const title = 'Made in Khon Kaen';
const description = 'This is a prompt page';
---

<Layout title={title} description={description}>
  <div class='text-center cover-trip bg-white'>
    <figure class="text-center">
      <img class="cover-photo _redius w-full object-cover" loading="lazy" src={srcImage.src} alt=""/>
    </figure>
    <div class="text-center md:hidden p-6">
      <img class="m-auto h-12 rounded-full" style="margin-top: -100px;" src={Logo.src} alt=""/ loading="lazy">
      <h1 class="mt-12 mb-10 font-handwriting font-semibold text-6xl text-gray-900">Made in <br> Khon Kaen</h1>
      <p class="pl-6 pr-6">ตามรอยเส้นทางวัตถุดิบ <br> Made in Khon Kaen</p>
    </div>
  </p>
  </div>

  <div class="max-w-4xl p-6 mx-auto grid grid-cols-1 lg:max-w-5xl lg:gap-x-20 lg:grid-cols-2">
    <div class="relative p-3 col-start-1 row-start-1 flex flex-col-reverse rounded-lg bg-gradient-to-t from-black/75 via-black/0 sm:bg-none sm:row-start-2 sm:p-0 lg:row-start-1">
